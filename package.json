{
	"name": "highlight-bot",
	"version": "3.0.0",
	"description": "An utility bot written in Sapphire to get highlighted when a word, or regular expression is said",
	"private": true,
	"main": "dist/Highlight.js",
	"type": "module",
	"imports": {
		"#internals/*": "./dist/lib/internals/*.js",
		"#hooks/*": "./dist/lib/utils/hooks/*.js",
		"#setup": "./dist/lib/setup.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#utils/*": "./dist/lib/utils/*.js",
		"#workers/*": "./dist/lib/workers/*.js"
	},
	"scripts": {
		"build": "tsc",
		"clean": "rimraf dist",
		"cleanbuild": "yarn clean && tsc",
		"coverage": "vitest run --coverage",
		"lint": "eslint --fix src --ext ts",
		"start": "yarn cleanbuild && node .",
		"test": "vitest run",
		"watch": "tsc -w"
	},
	"dependencies": {
		"@discordjs/builders": "^1.6.5",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"@prisma/client": "^5.2.0",
		"@sapphire/decorators": "^6.0.2",
		"@sapphire/discord.js-utilities": "^7.0.2",
		"@sapphire/framework": "^4.5.3",
		"@sapphire/plugin-logger": "^3.0.6",
		"@sapphire/plugin-subcommands": "^4.2.1",
		"@sapphire/stopwatch": "^1.5.0",
		"@sapphire/time-utilities": "^1.7.10",
		"@sapphire/type": "^2.4.0",
		"@sapphire/utilities": "^3.13.0",
		"@skyra/env-utilities": "^1.2.1",
		"@skyra/jaro-winkler": "^1.1.0",
		"bufferutil": "^4.0.7",
		"colorette": "^2.0.20",
		"confusables": "^1.1.1",
		"discord-api-types": "^0.37.56",
		"discord.js": "^14.13.0",
		"re2": "^1.20.3",
		"tslib": "^2.6.2",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.8"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^5.0.2",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.0",
		"@types/node": "^20.5.7",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"@vitest/coverage-c8": "^0.33.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"prettier": "^3.0.3",
		"prisma": "^5.2.0",
		"rimraf": "^5.0.1",
		"typescript": "^5.2.2",
		"vitest": "^0.30.1"
	},
	"author": {
		"name": "Vlad Frangu",
		"email": "kingdgrizzle@gmail.com"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"packageManager": "yarn@3.5.1"
}
